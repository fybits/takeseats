    "@parcel/transformer-sass": "2.12.0",
    "gh-pages": "^6.1.1",
    "parcel": "^2.12.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "peerjs": "^1.5.2",
    "pixi-filters": "^6.0.0",
    "pixi.js": "^8.0.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svgo": "2.8.0"
      }
    }
  }
